{
  "name": "VerizonVideoPartnerSDK",
  "version": "1.5.1",
  "summary": "Verizon Video Partner SDK",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "swift_versions": "4.2",
  "description": "A native iOS SDK that makes it easy to play and monetize videos from the Verizon Video Partner network on iOS-based platforms. \nYou can find all details and tutorials in our repository https://github.com/VerizonAdPlatforms/VerizonVideoPartnerSDK-iOS/.",
  "homepage": "https://github.com/VerizonAdPlatforms/VerizonVideoPartnerSDK-iOS",
  "authors": {
    "James Lou": "jlou@verizonmedia.com",
    "Andrey Moskvin": "andrey.moskvin@verizonmedia.com",
    "Roman Tysiachnik": "roman.tysiachnik@verizonmedia.com",
    "Vladyslav Anokhin": "vladyslav.anokhin@verizonmedia.com"
  },
  "source": {
    "git": "https://github.com/VerizonAdPlatforms/VerizonVideoPartnerSDK-iOS.git",
    "tag": "1.5.1"
  },
  "source_files": "sources/**/*.swift",
  "exclude_files": [
    "sources/utils/Utils.swift",
    "sources/utils/Recorder.swift",
    "sources/utils/ActionComparator.swift",
    "sources/utils/MockTimer.swift",
    "sources/**/*Test*",
    "sources/**/*Spec*",
    "sources/**/Contents.swift"
  ],
  "ios": {
    "exclude_files": "sources/default controls",
    "dependencies": {
      "PlayerControls": [
        "2.0.3"
      ]
    },
    "frameworks": "WebKit",
    "vendored_frameworks": "OMSDK_Verizonmedia.framework"
  },
  "tvos": {
    "exclude_files": [
      "sources/custom controls",
      "sources/vpaid",
      "sources/metrics/open measurement"
    ]
  },
  "static_framework": true,
  "dependencies": {
    "VideoRenderer": [
      "1.28"
    ],
    "PlayerCore": [
      "1.1.3"
    ]
  },
  "platforms": {
    "ios": "9.0",
    "tvos": "9.0"
  },
  "frameworks": [
    "CoreMedia",
    "AVFoundation",
    "CoreGraphics"
  ],
  "swift_version": "4.2"
}
